{
  "name": "ngcp-rest-api",
  "version": "0.0.1",
  "description": "REST API for the NGCP platform",
  "productName": "NGCP REST API",
  "author": "",
  "private": true,
  "license": "GPL-3+",
  "os": [
    "linux"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "rimraf prod && webpack",
    "build:dev-prod": "rimraf prod && NODE_ENV=development webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf dist && NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node prod/server",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "9.1.2",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.1",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.1.1",
    "@nestjs/schedule": "2.1.0",
    "@nestjs/swagger": "6.1.2",
    "@nestjs/typeorm": "9.0.1",
    "@types/bcrypt": "5.0.0",
    "@types/bluebird": "3.5.36",
    "@types/express": "4.17.14",
    "@types/jsonwebtoken": "8.5.9",
    "@types/multer": "1.4.7",
    "@types/node": "18.7.18",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/validator": "13.7.6",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "express": "4.18.1",
    "fast-json-patch": "3.1.1",
    "generate-password": "1.7.0",
    "halson": "3.0.0",
    "mysql2": "2.3.3",
    "nest-winston": "1.7.0",
    "passport": "0.6.0",
    "passport-http": "0.3.0",
    "passport-http-header-strategy": "1.1.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "^7.4.0",
    "sd-notify": "2.8.0",
    "swagger-ui-express": "4.5.0",
    "telnet-client": "2.0.4",
    "typeorm": "0.3.10",
    "uuid": "9.0.0",
    "winston": "3.8.2",
    "winston-syslog": "2.6.0",
    "yargs": "17.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.8",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "9.1.1",
    "@types/jest": "28.1.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "10.2.4",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "ignore-loader": "0.1.2",
    "jest": "28.1.3",
    "node-gyp": "9.1.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "selfsigned": "2.1.1",
    "string-replace-loader": "3.1.0",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typedoc": "0.22.18",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "optionalDependencies": {},
  "resolutions": {
    "**/ua-parser-js": "0.7.24"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
